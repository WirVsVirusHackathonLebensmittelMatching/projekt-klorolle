<mvc:View 
	controllerName="com.wir.vs.virus.timeslots.Consumer.controller.FastLaneTypeSelect"
	xmlns:core="sap.ui.core" 
	xmlns:l="sap.ui.layout" 
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true"
	xmlns="sap.m">
		<Page id="page" title="{i18n>nextFastLane}">
			<content>
				<VBox alignItems="Center">
					<Label text="{i18n>fastLaneConfirmed}" />
					<Label text="{/order/dateStart}" />
					<Label text="{/order/id}" />
					<Label text="{i18n>fastLaneConfirmSeeYou}" />
				</VBox>
			</content>
		</Page>
</mvc:View>