<mvc:View 
	controllerName="com.wir.vs.virus.timeslots.Consumer.controller.FastLaneConfirm"
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true"
	xmlns="sap.m">
		<Page id="page" title="{i18n>fastLaneConfirmedTitle}">
			<content>
				<VBox alignItems="Center" class="sapUiLargeMarginTop">
					<Label text="{i18n>fastLaneConfirmed}" />
					<Label text="{account>/order/dateStart}" design="Bold" class="sapUiSmallMarginTop"/>
					<Label text="{account>/order/id}" design="Bold" class="sapUiSmallMarginTop" />
					<Label text="{i18n>fastLaneConfirmSeeYou}" class="sapUiSmallMarginTop" />
				</VBox>
			</content>
		</Page>
</mvc:View>