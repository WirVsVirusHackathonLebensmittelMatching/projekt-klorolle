<mvc:View 
	controllerName="com.wir.vs.virus.timeslots.Consumer.controller.FastLaneConfirm"
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true"
	xmlns="sap.m">
		<Page id="page" title="{i18n>fastLaneConfirmedTitle}" showNavButton="true" navButtonPress="onNavBack">
			<content>
				<VBox alignItems="Center" class="sapUiLargeMarginTop">
					<Label text="{i18n>fastLaneConfirmed}" />
					<Label text="{path: 'account>/order/dateStart', formatter:'.formatDate'}" design="Bold" />
					<Label text="{i18n>fastLaneId}"  class="sapUiSmallMarginTop" />
					<Label text="{account>/order/id}" design="Bold" />
					<Label text="{i18n>fastLaneConfirmSeeYou}" class="sapUiSmallMarginTop" />
				</VBox>
			</content>
		</Page>
</mvc:View>